processCards =
    List.indexedMap
        (\i ( title, partialLink, summary ) ->
            let
                viewport =
                    { x = 0.0
                    , y = toFloat i * testViewport.height + (toFloat i * 15 * 6) + 50
                    , w = testViewport.width
                    , h = testViewport.height
                    }
            in
            ( title
            , ( { lang = partialLink.lang
                , title = partialLink.title
                , domElement = E.int i
                , rect =
                    { x = partialLink.linkPos.x
                    , y = partialLink.linkPos.y
                    , width = linkDimensions.width
                    , height = linkDimensions.height
                    , top = partialLink.linkPos.y
                    , bottom = (viewport.y + viewport.h) - (viewport.y + partialLink.linkPos.y + linkDimensions.height)
                    , left = partialLink.linkPos.x
                    , right = (viewport.x + viewport.w) - (viewport.x + partialLink.linkPos.x + linkDimensions.width)
                    }
                , viewport =
                    { scene =
                        { width = 1000
                        , height = 10000
                        }
                    , viewport =
                        { x = viewport.x
                        , y = viewport.y
                        , width = viewport.w
                        , height = viewport.h
                        }
                    }
                }
              , D.decodeString decodeSummary summary |> Debug.log "summary" |> Result.toMaybe
              , False
              )
            )
        )


main =
    div []
        [ node "style" [] [ text Card.styles ]
        , ul
            [ style "columns" "3"
            , style "font-size" "14px"
            , style "list-style-position" "inside"
            , style "list-style-type" "none"
            ]
          <|
            List.map viewIndex cards
        , div [ style "position" "relative" ]
            [ div [ style "height" (px ((List.length cards |> toFloat) * testViewport.height)) ] <|
                List.map viewViewport cards
            , div [] <| List.map viewCard cards
            ]
        ]


titleLinkId title link =
    title ++ " - " ++ link.title


viewIndex : ( String, ( Link, Maybe Summary, Bool ) ) -> Html ()
viewIndex ( title, ( link, maybeSummary, dismissed ) ) =
    li
        [ style "text-overflow" "ellipsis"
        , style "overflow" "hidden"
        , style "word-break" "break-all"
        , style "white-space" "nowrap"
        ]
        [ a
            [ href ("#" ++ titleLinkId title link)
            , style "color" "#005fc7"
            ]
            [ text <| titleLinkId title link ]
        ]


viewViewport : ( String, ( Link, Maybe Summary, Bool ) ) -> Html ()
viewViewport ( title, ( link, maybeSummary, dismissed ) ) =
    div
        [ style "position" "absolute"
        , style "top" (px link.viewport.viewport.y)
        , style "left" (px link.viewport.viewport.x)
        , style "width" (px link.viewport.viewport.width)
        , style "height" (px link.viewport.viewport.height)
        , style "background-color" "white"
        , style "box-shadow" "0px 2px 2px rgba(0,0,0,0.1)"
        ]
        [ a
            [ id <| titleLinkId title link
            , style "position" "absolute"
            , style "top" "-30px"
            , style "left" "10px"
            , style "width" (px link.viewport.viewport.width)
            , style "height" (px link.viewport.viewport.height)
            , style "color" "black"
            , style "font-weight" "bold"
            , style "font-size" "16px"
            ]
            [ text <| titleLinkId title link ]
        , a
            [ style "position" "absolute"
            , style "top" (px link.rect.top)
            , style "left" (px link.rect.left)
            , style "width" (px link.rect.width)
            , style "height" (px link.rect.height)
            , style "background-color" "#ffd5d5"
            , style "box-shadow" "0px 5px 20px rgba(0,0,0,0.1)"
            , style "border-radius" "3px"
            ]
            []
        ]


viewCard : ( String, ( Link, Maybe Summary, Bool ) ) -> Html ()
viewCard ( title, ( link, maybeSummary, dismissed ) ) =
    Card.view events link maybeSummary dismissed


events : Card.Events ()
events =
    { mouseEnter = \_ -> ()
    , mouseLeave = \_ -> ()
    }


px : Float -> String
px n =
    String.fromFloat n ++ "px"


